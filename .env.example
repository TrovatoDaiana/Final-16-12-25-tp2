# Copiar a .env.dev y completar valores reales
NODE_ENV=development
PORT=3000
DB_PROVIDER=mongo
# Formato sugerido para Atlas (reemplazar placeholders)
MONGO_URI=mongodb+srv://<USER>:<PASS>@<CLUSTER>/<DB_NAME>
# Clave de firma para JWT (usar una cadena cualquiera en dev)
JWT_SECRET=dev-jwt-secret
# Estrategia de autenticaci√≥n: jwt | api-key | none
AUTH_STRATEGY=jwt
# API Key opcional si usas api-key
API_KEY=<OPCIONAL_API_KEY>
